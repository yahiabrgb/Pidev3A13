    <script>
        $("#rechercherrec").change(
            function () {

                var recherche=$('#rechercherrec').val();
                var DATA = 'recherche='+recherche;
                $.ajax({
                    type:"POST",
                    url:"{{ path('recherche') }}",
                    data:DATA,
                    success:function(data) {
                        console.log(data);
                        $("#afficher").html("<div id='res'>"+"</div>"
                        );
                        $.each(data,function (k,el) {
                            //URL = URL.replace("username", el.user);
                            URL = URL.replace("type", el.type);
                            URL = URL.replace("dateCreation", el.dateCreation);
                            URL = URL.replace("adresse", el.adresse);
                            $('#res').append(

                                "<tr>"+
                                    "<td>"+el.type +"</td>"+
                                    "<td>"+el.dateCreation +"</td>"+
                                "</tr>"+
                                "</div>"+"</div>"
                            );
                        });
                    }
                });
            }
        );
    </script>
