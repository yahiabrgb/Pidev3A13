                                                                {% elseif "now" |date("U") + 3600 > commentaire.dateCommentaire |date("U")  %}
                                                                    {{ ("now" |date("U") + 3600 - commentaire.dateCommentaire |date("U") ) |date("s") }} Seconds Ago
                                                                {% endif %}
                                                            </span>
                                                                                    <p id="comment_content-{{ commentaire.id }}">{{ commentaire.commentaire }}</p>
                                                                                </div>
                                                                            </div><!--comment-list end-->
                                                                        </li>
                                                                    {% endif %}
                                                                {% endfor %}

                                                            </div><!--comment-sec end-->
                                                        </ul>
                                                        <div class="post-comment">
                                                            <div class="cm_img">
                                                                <img src="{{ asset('/template/images/resources/bg-img4.png')}}" alt="">
                                                            </div>
                                                            <div class="comment_box">
                                                                <form onsubmit="return false;">
                                                                    <input id="new_comment-{{publication.id}}" type="text" placeholder="Post a comment">
                                                                    {% if user %}
                                                                        <button class="form"  id="submit_new_comment-{{publication.id}}" type="submit">Send</button>
                                                                    {% else %}
                                                                        <a href="{{ path('fos_user_security_login') }}" >Login !</a>
                                                                    {% endif %}
                                                                </form>
                                                            </div>
                                                        </div><!--post-comment end-->
                                                    </div><!--comment-section end-->
                                                </div>
                                            </div><!--posty end-->
                                </div><!--posts-section end-->
