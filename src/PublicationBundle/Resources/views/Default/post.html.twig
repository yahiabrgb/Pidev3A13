                                                        <a href="#" class="plus-ic">
                                                            <i class="la la-plus"></i>
                                                        </a>
                                                        <ul>
                                                            <div class="comment-sec" id="comment-sec-{{publication.id}}">

                                                                {% for commentaire in commentaires %}
                                                                    {%  if commentaire.Publicationid.id == publication.id %}
