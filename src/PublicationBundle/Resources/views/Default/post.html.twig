                                                                <li><a href="{{ path('publication_show_hashtag', {'hashtag': categorie }) }}" title="">{{categorie}}</a></li>
                                                            {% endfor %}
                                                        </ul>
                                                    </div>
                                                    {{ socialButtons( { 'facebook': {'locale':'fr_FR', 'url':   'http://127.0.0.1:8000/post/' ~ publication.id }}  ,{ 'twitter': {'locale':'fr_FR', 'url':  'http://localhost:8000/post/' ~ publication.id } } ) }}
