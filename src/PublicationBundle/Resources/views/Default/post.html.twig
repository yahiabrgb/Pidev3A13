                                                            <li><a href="#" title=""><i class="la la-envelope"></i></a></li>
                                                        </ul>
                                                    </div>
                                                    <div class="job_descp">
                                                        <h3 id="titre_publication-{{ publication.id }}" >{{publication.titrePublication}}</h3>
                                                        <input type="hidden" id="type_publication-{{ publication.id }}" value="{{ publication.typePublication }}" />
