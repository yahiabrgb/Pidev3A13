                                                            <center><video width="415" controls>
                                                                    <source src="{{ asset('/uploads/video/') ~ publication.srcPublication }}" type="video/mp4">
                                                                    Your browser does not support HTML5 video.
                                                                </video></center>
                                                        {% elseif publication.typePublication == "audio" %}
